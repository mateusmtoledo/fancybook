{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "./backend/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "./frontend/build",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/app.js"
    }
  ]
}
