{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-shadow": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id", "__v", "_json"]
      }
    ]
  },
  "plugins": ["jest"]
}
